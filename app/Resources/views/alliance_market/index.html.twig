{% extends 'secure_base.html.twig' %}{% block body %}    {% if transactionType == 'P' %}        {% set marketText = 'Sell' %}        {% set marketIcon = 'fa-money' %}        {% set cartIcon = 'fa-cart-plus' %}        {% set infoText = 'Income' %}    {% elseif transactionType == 'S' %}        {% set marketText = 'Buy' %}        {% set marketIcon = 'fa-credit-card' %}        {% set cartIcon = 'fa-cart-arrow-down' %}        {% set infoText = 'Cost' %}    {% endif %}    <div class="row">        <div class="col-md-12">            <div class="box">                <div class="box-header with-border">                    <h3 class="box-title"><i class="fa {{ marketIcon }} "></i>  {{ marketText }}</h3>                </div>                <div class="box-body">                    <div class="row" style="margin-top:20px; margin-bottom:20px;">                        <div class="col-md-3">                            <div class="info-box">                                <span class="info-box-icon bg-green"><i class="fa {{ cartIcon }}"></i></span>                                <div class="info-box-content">                                    <span class="info-box-text">Pending {{ marketText }} Orders</span>                                    <span class="info-box-number" id="pendingSales">{{transactionSummary.totalTransactionPending|number_format(0, '.', ',')}}</span>                                    <span class="text-muted"></span>                                </div><!-- /.info-box-content -->                            </div>                        </div>                        <div class="col-md-3">                            <div class="info-box">                                <span class="info-box-icon bg-gray"><i class="fa {{ cartIcon }}"></i></span>                                <div class="info-box-content">                                    <span class="info-box-text">Completed {{ marketText }} Orders</span>                                    <span class="info-box-number" id="completedSales">{{transactionSummary.totalTransactionAccepted|number_format(0, '.', ',')}}</span>                                    <span class="text-muted"></span>                                </div><!-- /.info-box-content -->                            </div>                        </div>                        <div class="col-md-3">                            <div class="info-box">                                <span class="info-box-icon bg-green"><i class="fa {{ marketIcon }}"></i></span>                                <div class="info-box-content">                                    <span class="info-box-text">Pending {{ infoText }}</span>                                    <span class="info-box-number" id="pendingIncome">{{transactionSummary.totalNetPending|number_format(0, '.', ',')}}</span>                                    <span class="text-muted"></span>                                </div><!-- /.info-box-content -->                            </div>                        </div>                        <div class="col-md-3">                            <div class="info-box">                                <span class="info-box-icon bg-gray"><i class="fa {{ marketIcon }}"></i></span>                                <div class="info-box-content">                                    <span class="info-box-text">Completed {{ infoText }}</span>                                    <span class="info-box-number" id="completedIncome">{{transactionSummary.totalNetAccepted|number_format(0, '.', ',')}}</span>                                    <span class="text-muted"></span>                                </div><!-- /.info-box-content -->                            </div>                        </div>                    </div>                    <div class="row">                        <div class="col-md-6">                            {% if eveCentralOK == true %}                                {% form_theme form 'forms/my_bootstrap_layout_no_label.html.twig' %}                                {{ form_start(form) }}                                {{ form_row(form.items, {'attr': {'placeholder': 'Paste items from game here', 'style': 'height:400px;'}}) }}                                <div id="alliance_market_submits" class="right_justify_submits">                                    <a class="btn btn-info pull-left" href="{{ path('pages', {'chapter_slug': 'help', 'topic_slug': 'help_using_alliance_market'}) }}" target="_new">Help</a>                                    <input type="button" id="alliance_market_clear" class="btn btn-danger " value="Clear">                                    <input type="button" id="alliance_market_quick_submit" class="btn btn-success " value="Get Estimate">                                </div>                                {{ form_end(form) }}                            {% else %}                                <div class="alert alert-danger alert-dismissable">                                    <h4><i class="icon fa fa-ban"></i> EveCentral is Down!</h4>                                    The {{ systemNameShort|raw }} market relies on EveCentral for statistical pricing information.  Due to EveCentral being down, {{ systemNameShort|raw }} will not accept buyback contracts.                                </div>                            {% endif %}                        </div>                        <div class="col-md-6">                            <table class="table table-striped" id="rTransactions">                                <thead>                                    <tr>                                        <th>Date</th>                                        <th>Order ID</th>                                        <th>Isk Amount</th>                                        <th>Status</th>                                    </tr>                                </thead>                                <tbody>                                    {% for transaction in oTransaction %}                                        {% if transaction.status == 'Pending' %}                                            <tr class="success" id="row-{{transaction.orderid}}">                                        {% elseif transaction.status == 'Cancelled' %}                                            <tr class="danger" id="row-{{transaction.orderid}}">                                        {% else %}                                            <tr class="bg-gray" id="row-{{transaction.orderid}}">                                        {% endif %}                                                <td>{{transaction.created|date("m/d/Y H:i")}}</td>                                                <td><a class="fake_link" data-type="{{transaction.type}}">{{transaction.orderid}}</a></td>                                                <td>{{transaction.net|number_format(0, '.', ',')}}</td>                                                <td>                                                    {% if transaction.status == 'Pending' %}                                                        <span class="label label-success pull-right">{{transaction.status}}</span>                                                    {% elseif transaction.status == 'Cancelled' %}                                                        <span class="label label-danger pull-right">{{transaction.status}}</span>                                                    {% else %}                                                        <span class="label label-grey pull-right">Complete</span>                                                    {% endif %}                                                </td>                                            </tr>                                    {% endfor %}                                </tbody>                            </table>                        </div>                    </div>                </div>            </div>        </div>    </div>    <div class="modal fade" id="index_modal">      <div class="modal-dialog">        <div class="modal-content" id="index_modal_content">        </div><!-- /.modal-content -->      </div><!-- /.modal-dialog -->    </div><!-- /.modal -->    <div class="modal fade" id="estimate_modal">      <div class="modal-dialog" id="estimate_modal_content">        <div class="modal-content">            <div class="modal-loading"><i class="fa fa-refresh fa-spin"></i></div>        </div><!-- /.modal-content -->      </div><!-- /.modal-dialog -->    </div><!-- /.modal -->{% endblock %}{% block stylesheets %}{% endblock %}{% block javascripts %}    <script type="text/javascript">        $(document).ready(function (e)        {            $('.data-table').DataTable();            $('#rTransactions').DataTable(                {                    "order": [[ 0, "desc" ]],                    // Define custom handler for createdRow event                    "createdRow" : function( row, data, index )                    {                        // Add identity if it specified                        if( data.hasOwnProperty("id") )                        {                            row.id = data.id;                        }                        // Add class if it specified                        if( data.hasOwnProperty("className") )                        {                            row.className = data.className;                        }                    },                    "fnDrawCallback": function( oSettings )                    {                        bindClickFakeLink();                    }                });            $(".news_item_link").click(function(e)            {                $('#index_modal').modal({backdrop: "static"});                $.get('{{path('ajax_view_news')}}', {id: $(this).attr('data-i')},                    function(response)                    {                        $( "#index_modal_content" ).html( response );                    }                    , "html");            });            $("#alliance_market_quick_submit").click(function(e)            {                $('#base_modal').modal({backdrop: "static"});                $("#base_modal_content").html("<div class='modal-loading' style='font-size:52px;'><i class='fa fa-refresh fa-spin'></i></div>")                $.post('{{path('ajax_alliance_market_estimate')}}', {'alliance_market_form[items]': $("#alliance_market_form_items").val(), 'transactionType': '{{ transactionType }}' },                    function(response)                    {                        $( "#base_modal_content" ).html( response );                    }                    , "html");            });            $("#alliance_market_clear").click(function(e)            {                $("#alliance_market_form_items").val('');            });        });        function bindClickFakeLink()        {            $(".fake_link[data-type='{{ transactionType }}']").click(function(e)            {                //put spinner in modal                $('#base_modal').modal({backdrop: "static"});                $('#base_modal_content').html('<div class="modal-loading"><i class="fa fa-refresh fa-spin"></i></div>');                $.get('{{path('ajax_view_transaction')}}', {id: $(this).html(), transaction_type: $(this).attr("data-type")},                    function(response)                    {                        $( "#base_modal_content" ).html( response );                    }                    , "html");            });        }    </script>{% endblock %}